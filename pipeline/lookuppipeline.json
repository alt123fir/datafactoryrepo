											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "adlstoblbmeta",
									"type": "DatasetReference",
									"parameters": {
										"filename": {
											"value": "@concat(item().TABLE_NAMES,'.csv')",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
